{
  "name": "Minimal Scrollbars",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Make your scrollbars look standard & minimal",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles.css"]
    }
  ]
}
